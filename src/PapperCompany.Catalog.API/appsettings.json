{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "CorsSettings": {
    "Policy": {
      "Name": "api-catalog-policy",
      "AllowedOrigins": [ "*" ],
      "AllowedMethods": [ "GET", "POST", "PUT", "DELETE" ],
      "AllowedHeaders": [ "Authorization", "Content-Type" ]
    }
  },

  "VersioningSettings": {
    "ApiVersion": {
      "High": 1,
      "Medium": 0
    },
    "Reader": "x-api-version",

    "Explorer": {
      "Format": "'v'VVV"
    }
  }
}
